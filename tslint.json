{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "no-console": false,
    "member-access": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      true,
      150
    ],
    "max-classes-per-file": false,
    "member-ordering": [
      false
    ],
    "interface-name": [
      true
    ],
    "arrow-parens": false,
    "semicolon": [
      true,
      "never"
    ],
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    },
    "prefer-readonly": true,
    "object-literal-sort-keys": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "never"
        },
        "esSpecCompliant": true
      }
    ]
  },
  "rulesDirectory": []
}
{
}
